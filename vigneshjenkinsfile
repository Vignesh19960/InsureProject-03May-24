pipeline {
  agent any

  tools {
    maven 'M2_HOME'
    }
  
  stages {
    stage('Checkout') {
      steps {
        echo 'Checkout the source code from GitHub'
        git branch: 'main', url: 'https://github.com/Vignesh19960/InsureProject-03May-24.git'
            }
    }
    stage('Package') {
      steps {
        echo 'Create a Package'
        sh 'mvn clean package'
             }
          }
      }
}
